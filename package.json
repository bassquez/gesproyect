{
  "name": "gesproyect",
  "version": "1.0.0",
  "license": "MIT",
  "env": {
    "NODE_ENV": "production"
  },
  "engines": {
    "node": "8.10.0",
    "npm": "6.2.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "node dist/server/app.js",
    "build": "npm run build:prod && tsc -p server",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "build:prod": "ng build --prod --build-optimizer --vendor-chunk --common-chunk",
    "clean": "rimraf ./dist/",
    "server": "cd dist && http-server",
    "ci": "npm run clean && npm run prettier:ci && ng lint && ng test --configuration=test --browsers ChromeTravisCi && ng e2e && npm run build:prod -- --deploy-url /gesproyect/ --base-href /gesproyect",
    "release": "standard-version && git push --follow-tags origin master",
    "prettier": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "prettier:ci": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "analyze": "npm run clean && npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/stats.json",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open --host 0.0.0.0\" \"tsc -w -p server\" \"nodemon dist/server/app.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && tsc -p server && node dist/server/app.js\"",
    "test": "ng test",
    "testbe": "tsc -p server && mocha dist/server/test --exit",
    "lint": "ng lint",
    "lintbe": "tslint server/**/**.ts{,x}",
    "e2e": "ng e2e",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular/animations": "6.0.0",
    "@angular/cdk": "^6.2.0",
    "@angular/cli": "^6.0.8",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/material": "^6.2.0",
    "@angular/material-moment-adapter": "^6.4.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "6.0.0",
    "@auth0/angular-jwt": "^2.0.0",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@ngrx/effects": "^6.0.0-beta.1",
    "@ngrx/store": "^6.0.0-beta.1",
    "angular-froala-wysiwyg": "^2.8.4-1",
    "angular2-moment": "^1.9.0",
    "angular2-text-mask": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.0.0",
    "browser-detect": "^0.2.27",
    "chart.js": "^2.7.2",
    "core-js": "^2.4.1",
    "dhtmlx-gantt": "^5.2.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "gantt-ui-component": "^0.2.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.2.1",
    "moment": "^2.22.2",
    "mongoose": "^5.1.2",
    "morgan": "^1.9.0",
    "ng2-charts": "^1.6.0",
    "ng2-google-charts": "^3.4.0",
    "node-sass": "^4.9.4",
    "popper.js": "^1.14.3",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.2.0",
    "serve-favicon": "^2.5.0",
    "tether": "^1.4.4",
    "uuid": "^3.1.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/express": "^4.11.1",
    "@types/jasmine": "~2.8.3",
    "@types/node": "~9.4.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codelyzer": "~4.1.0",
    "concurrently": "^3.5.1",
    "enhanced-resolve": "~3.4.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-spec-reporter": "~0.0.31",
    "mocha": "^5.2.0",
    "ngrx-store-freeze": "^0.2.1",
    "nodemon": "^1.17.5",
    "prettier": "^1.7.4",
    "protractor": "^5.4.1",
    "rimraf": "^2.6.2",
    "standard-version": "^4.2.0",
    "ts-node": "~5.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "webpack-bundle-analyzer": "^2.11.1"
  }
}
